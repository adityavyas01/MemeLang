hi_bhai
rakho x = 10;
rakho y = 20;

agar (x < y) {
  chaap x + " is less than " + y;
} nahi_to {
  chaap x + " is greater than or equal to " + y;
}

rakho i = 0;
jab_tak (i < 5) {
  chaap "Count: " + i;
  i = i + 1;
}

rakho j = 0;
jab_tak (j < 10) {
  agar (j == 5) {
    j = j + 1;
    continue;
  }
  chaap "Number: " + j;
  j = j + 1;
}

rakho k = 0;
jab_tak (k < 10) {
  agar (k == 7) {
    break;
  }
  chaap "Value: " + k;
  k = k + 1;
}
bye_bhai 